data <- read.csv ("pDat.csv")
BMI <- data$BMI
# Q1
Mean <- mean(BMI)
stanDiv <- sqrt((sum(BMI**2) - ((sum(BMI))**2)/length(BMI))/length(BMI))
# Q2
s1 <- read.csv ("rs1.csv")
s1M <- mean(s1$BMI)
s1SD <- sd(s1$BMI)
s2 <- read.csv ("rs2.csv")
s2M <- mean(s2$BMI)
s2SD <- sd(s2$BMI)
s3 <- read.csv ("rs3.csv")
s3M <- mean(s3$BMI)
s3SD <- sd(s3$BMI)
s4 <- read.csv ("rs4.csv")
s4M <- mean(s4$BMI)
s4SD <- sd(s4$BMI)
s5 <- read.csv ("rs5.csv")
s5M <- mean(s5$BMI)
s5SD <- sd(s5$BMI)
s6 <- read.csv ("rs6.csv")
s6M <- mean(s6$BMI)
s6SD <- sd(s6$BMI)
s7 <- read.csv ("rs7.csv")
s7M <- mean(s7$BMI)
s7SD <- sd(s7$BMI)
s8 <- read.csv ("rs8.csv")
s8M <- mean(s8$BMI)
s8SD <- sd(s8$BMI)
s9 <- read.csv ("rs9.csv")
s9M <- mean(s9$BMI)
s9SD <- sd(s9$BMI)
s10 <- read.csv ("rs10.csv")
s10M <- mean(s10$BMI)
s10SD <- sd(s10$BMI)
# Q3
sMeans <- c(s1M, s2M, s3M, s4M, s5M, s6M, s7M, s8M, s9M, s10M)
meanOfSample <- mean(sMeans)
stanDivOfSample <- (stanDiv/sqrt(5)) * sqrt(35/39)
# Q5
samOf15 <- read.csv("rs15.csv")
samBMI <- samOf15$BMI
mean(samBMI)
CI(samBMI, ci=0.95)
